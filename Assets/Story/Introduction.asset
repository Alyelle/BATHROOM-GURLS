    speakerSpriteId: 0
    speaker: {fileID: 11400000, guid: d632a88962f871444aebd2b5cce93ada, type: 2}
    voiceLine: {fileID: 0}
  - text: "Is it that \u201Cmonsters on the way to the bathroom\u201D rumor again?"
